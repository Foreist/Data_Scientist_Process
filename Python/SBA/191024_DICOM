lf.filelist = glob.glob(os.path.join(data_dir, \"*.DCM\"))\n","    \n","  def __len__(self):\n","    return len(self.filelist)\n","    \n","  def __getitem__(self, idx):\n","    dcm_data = pydicom.read_file(self.filelist[idx])\n","    imgs = dcm_data.pixel_array\n","    assert imgs.ndim == 3\n","    \n","    # see: https://pytorch.org/docs/stable/nn.html#conv3d\n","    imgs = np.expand_dims(imgs, axis=1)\n","    assert imgs.shape == (96, 1, 512, 512)\n","    \n","    # augmentation    \n","    return imgs"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"SBvyHcRMRCkn","colab_type":"code","colab":{}},"source":["dataset = CustomDataset(data_dir)"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"otnPaxkTT-X4","colab_type":"code","outputId":"0834d28a-34c9-4349-e853-1849cf2c6d76","executionInfo":{"status":"ok","timestamp":1571892373029,"user_tz":-540,"elapsed":56444,"user":{"displayName":"김태웅","photoUrl":"","userId":"06196795162879697734"}},"colab":{"base_uri":"https://localhost:8080/","height":35}},"source":["dataset.filelist"],"execution_count":15,"outputs":[{"output_type":"execute_result","data":{"text/plain":["['./0002.DCM', './0003.DCM']"]},"metadata":{"tags":[]},"execution_count":15}]},{"cell_type":"code","metadata":{"id":"oRlscESqTH-F","colab_type":"code","colab":{}},"source":["dataloader = DataLoader(dataset, batch_size=2)"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"ul5CuG3JTeAx","colab_type":"code","colab":{}},"source":["samples = iter(dataloader).next()"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"AzoRGSZHTrEn","colab_type":"code","outputId":"c182af5a-a3e4-447c-972f-54a39b43bafe","executionInfo":{"status":"ok","timestamp":1571892373868,"user_tz":-540,"elapsed":57245,"user":{"displayName":"김태웅","photoUrl":"","userId":"06196795162879697734"}},"colab":{"base_uri":"https://localhost:8080/","height":35}},"source":["samples.shape"],"execution_count":18,"outputs"